            <li><input type="password" class="text" id="pw2" placeholder="再 次 输 入 密 码"><span style="color:red;font-style:italic"></span></li>
            <li><p><button type="button" id='submit'><span>注 册</span></button></p><button type="button" id='cancel'><span>取 消</span></button></li>
            <li></li>
        </ul>
    </div>
    <script src="~/Content/js/jquery-1.11.3.min.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/Register.js"></script>
